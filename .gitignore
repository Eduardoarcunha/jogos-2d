# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/UnnamedGame/[Ll]ibrary/
/UnnamedGame/[Tt]emp/
/UnnamedGame/[Oo]bj/
/UnnamedGame/[Bb]uild/
/UnnamedGame/[Bb]uilds/
/UnnamedGame/[Ll]ogs/
/UnnamedGame/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/UnnamedGame/[Mm]emoryCaptures/

# Recordings can get excessive in size
/UnnamedGame/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /UnnamedGame/[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/UnnamedGame/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/UnnamedGame/.vs/

# Gradle cache directory
/UnnamedGame/.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/UnnamedGame/ExportedObj/
/UnnamedGame/.consulo/
/UnnamedGame/*.csproj
/UnnamedGame/*.unityproj
/UnnamedGame/*.sln
/UnnamedGame/*.suo
/UnnamedGame/*.tmp
/UnnamedGame/*.user
/UnnamedGame/*.userprefs
/UnnamedGame/*.pidb
/UnnamedGame/*.booproj
/UnnamedGame/*.svd
/UnnamedGame/*.pdb
/UnnamedGame/*.mdb
/UnnamedGame/*.opendb
/UnnamedGame/*.VC.db

# Unity3D generated meta files
/UnnamedGame/*.pidb.meta
/UnnamedGame/*.pdb.meta
/UnnamedGame/*.mdb.meta

# Unity3D generated file on crash reports
/UnnamedGame/sysinfo.txt

# Builds
/UnnamedGame/*.apk
/UnnamedGame/*.aab
/UnnamedGame/*.unitypackage
/UnnamedGame/*.app

# Crashlytics generated file
/UnnamedGame/crashlytics-build.properties

# Packed Addressables
/UnnamedGame/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/UnnamedGame/[Aa]ssets/[Ss]treamingAssets/aa.meta
/UnnamedGame/[Aa]ssets/[Ss]treamingAssets/aa/*
